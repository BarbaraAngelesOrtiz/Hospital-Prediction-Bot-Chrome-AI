{
  "manifest_version": 3,
  "name": "Hospital Bot Extension",
  "version": "1.0",
  "description": "Analyze hospital data and interact with a bot.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": [
    "storage",
    "activeTab"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}






